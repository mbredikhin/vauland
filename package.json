{
  "name": "vauland",
  "type": "module",
  "version": "0.0.0",
  "description": "Unstyled drawer component for Vue.",
  "keywords": [
    "vue",
    "vue3",
    "drawer",
    "dialog",
    "modal",
    "headless"
  ],
  "homepage": "https://github.com/mbredikhin/vauland",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mbredikhin/vauland.git"
  },
  "bugs": {
    "url": "https://github.com/mbredikhin/vauland/issues"
  },
  "author": "Maxim Bredikhin",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.6.0"
  },
  "packageManager": "pnpm@10.11.0",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.umd.cjs"
    }
  },
  "scripts": {
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "lint": "pnpm eslint"
  },
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typings": "./dist/index.d.ts",
  "files": [
    "./README.md",
    "./LICENSE",
    "./dist"
  ],
  "dependencies": {
    "@vueuse/core": "10.8.0",
    "aria-hidden": "1.2.4",
    "defu": "6.1.4"
  },
  "peerDependencies": {
    "vue": ">= 3.3.0"
  },
  "devDependencies": {
    "vue": "3.4.5",
    "@types/node": "18.19.3",
    "@vitejs/plugin-vue": "4.5.2",
    "@vue/eslint-config-typescript": "12.0.0",
    "@vue/tsconfig": "0.5.0",
    "typescript": "~5.3.0",
    "vite": "5.0.10",
    "vite-plugin-css-injected-by-js": "3.3.1",
    "vite-plugin-dts": "3.7.0",
    "vue-tsc": "1.8.25",
    "@eslint/js": "9.11.1",
    "@typescript-eslint/eslint-plugin": "8.7.0",
    "@typescript-eslint/parser": "8.7.0",
    "eslint": "9.11.1",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-vue": "9.32.0",
    "typescript-eslint": "8.7.0",
    "prettier": "3.3.3",
    "semantic-release": "24.2.5",
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "subject-empty": [
        0
      ]
    }
  },
  "release": {
    "branches": [
      "main"
    ]
  }
}